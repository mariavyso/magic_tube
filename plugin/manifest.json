{
  "manifest_version": 3,
  "name": "MagicTube",
  "version": "1.3",
  "description": "Allows to copy video ID, fixes some CSS",
  "host_permissions": ["*://*.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["simple-notify.min.js", "content.js"],
      "css": ["simple-notify.css"]
    }
  ],
  "icons": {
    "128": "icon_128.png"
  }
}
